      \strng{namehash}{aaa3b24189003c9f1ba687ace2530199}
      \strng{fullhash}{aaa3b24189003c9f1ba687ace2530199}
      \strng{fullhashraw}{aaa3b24189003c9f1ba687ace2530199}
      \strng{bibnamehash}{aaa3b24189003c9f1ba687ace2530199}
      \strng{editorbibnamehash}{aaa3b24189003c9f1ba687ace2530199}
      \strng{editornamehash}{aaa3b24189003c9f1ba687ace2530199}
      \strng{editorfullhash}{aaa3b24189003c9f1ba687ace2530199}
