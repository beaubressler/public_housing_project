      \strng{namehash}{a9d05bf0b8b979f3a3a09f722c1183b6}
      \strng{fullhash}{a9d05bf0b8b979f3a3a09f722c1183b6}
      \strng{fullhashraw}{a9d05bf0b8b979f3a3a09f722c1183b6}
      \strng{bibnamehash}{a9d05bf0b8b979f3a3a09f722c1183b6}
      \strng{authorbibnamehash}{a9d05bf0b8b979f3a3a09f722c1183b6}
      \strng{authornamehash}{a9d05bf0b8b979f3a3a09f722c1183b6}
