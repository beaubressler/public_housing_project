      \strng{namehash}{bf8127b3b11f226b88862b3dc050134c}
      \strng{fullhash}{bf8127b3b11f226b88862b3dc050134c}
      \strng{fullhashraw}{bf8127b3b11f226b88862b3dc050134c}
      \strng{bibnamehash}{bf8127b3b11f226b88862b3dc050134c}
